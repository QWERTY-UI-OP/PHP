<?php
$sum2=(100)*(101)/2;
$sum1=100*(201)*(101)/6;
$sum2=($sum2**2);
$esum=$sum2-$sum1;
echo "The difference between the sum of the squares of the first one hundred natural numbers and the square of the sum is = " . $esum;
