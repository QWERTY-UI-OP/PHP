<?php
function Prime(int $i){
    if($i==2){
        return 1;
    }
    for($a=2;$a<$i;$a++)
    {
        if($i%$a==0){
            return 0;
        }
        else{
            return 1;
        }
    }
}
$x=2;
$y=0;
while($y<=10001)
{
    if(Prime($x)){
        $y++;
    }
    $x++;
}
echo --$x;
?>
