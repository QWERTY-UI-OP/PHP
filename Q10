<?php
function prime($num) {
    if ($num < 2) {
        return 0;
    }
    for ($i = 2; $i <= sqrt($num); $i++) {
        if ($num % $i == 0) {
            return 0;
        }
    }
    return 1;
}
$sum = 0;
for ($num = 2; $num < 2000000; $num++) {
    if (prime($num)) {
        $sum += $num;
    }
}
echo "The sum of all prime numbers below two million is: $sum";
?>
