<?php

$num = "";

$max_product = 0;
for ($i = 0; $i < strlen($num) - 12; $i++) {
    $product = 1;
    for ($j = 0; $j < 13; $j++) {
        $product *= $num[$i + $j];
    }
    if ($product > $max_product) {
        $max_product = $product;
    }
}

echo "The greatest product of 13 adjacent digits is: $max_product";

?>
